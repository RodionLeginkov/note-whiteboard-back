<!DOCTYPE html>
<html>
    <head>
        <style>
        table {
            border-collapse: collapse;
        }
        h3{
            padding-bottom: 5px;
            margin-bottom: 5px;
        }
        table, th, td {
            border: 1px solid black;
        }
        </style>
    </head>
    <body>
        <div style="width: 85%; margin: 0 7.5%;">
            <div style="text-align: center; padding-left: 50px;">            
                <h3 style="font-size: 50px;">Your notes</h3>
                <% if (categories.length > 0) { %>
                    <% categories.forEach(category => {%>
                       <h2><%= category.title %></h2>
                        <svg style="max-width: 300px; max-height:300px"><%- category.svg %></svg>
                    
                    <%  });%>
                <% }%>

             </div>
        </div>
    </body>
</html>